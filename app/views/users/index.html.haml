%h1 Users

.row
  .col-sm-3.col-md-3.pull-right
    = render 'search'

.panel.panel-default
  - if @users.size > 0
    %ul.list-group
      - @users.each do |user|
        %li.list-group-item
          = user_listing(user)
  - else
    .panel-body No users found...

= will_paginate @users, renderer: BootstrapPagination::Rails
